  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "vite --port 3000",
    "preview": "vite preview --port 3000",
    "build": "vite build --outDir dist",
    "test": "vitest watch",
    "coverage": "vitest run --coverage && open coverage/index.html",
    "format": "prettier --ignore-path .gitignore --write \"{lib,src,test}/**/*.{ts,tsx}\"",
    "validate": "run-p --aggregate-output validate:*",
    "validate:format": "prettier --ignore-path .gitignore --check \"{lib,src,test}/**/*.{ts,tsx}\"",
    "validate:lint": "eslint --color --ignore-path .gitignore --ext .ts,.tsx .",
    "validate:types": "tsc --pretty",
    "validate:tests": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "@jackfranklin/test-data-bot": "^1.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.1",
    "@types/react": "^16.13.1",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-axe": "^7.0.0",
    "jest-styled-components": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.27.2"
  }
}
